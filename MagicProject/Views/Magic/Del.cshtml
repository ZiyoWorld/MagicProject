@using MagicProject.Models;
@{
    Layout = null;
    OrderUser[] s = ViewBag.OrderUsers;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <title>Del</title>
</head>
<body>

    <h1 class="text-center">Welcome to Delete from Database</h1>

    <div class="container">
        <div class="row my-3">
            <div class="col-lg-6 offset-3">
                <table class="table table-hover">
                    <thead class="text-info">
                        <tr>
                            <th>ID</th>
                            <th>typeName</th>
                            <th>ComfortId</th>
                            <th>CountID</th>
                            <th>PayId</th>
                            <th>CostlavelID</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (OrderUser a in s)
                        {
                            <tr>
                                <td>@a.ID</td>
                                <td>@a.prf</td>
                                <td>@a.user</td>
                                <td>@a.pos</td>
                                <td>@a.frm</td>
                                <td>@a.occ</td>
                                <td><a href="~/Magic/DelConfirm?ID=@a.ID" class="badge bg-danger text-white text-decoration-none px-3 py-1 ">Delete</a></td>
                            </tr>

                        }
                    </tbody>

                </table>
                <button type="button" class="btn btn-primary"><a href="~/Magic/Index" class="text-white">Index</a></button>
            </div>

        </div>






</body>
</html>